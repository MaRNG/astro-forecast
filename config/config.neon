# Core Config
includes:
	# Application
	- app/parameters.neon
	- app/services.neon

	# Extensions
	- ext/apitte.neon
	- ext/contributte.neon

php:
	date.timezone: Europe/Prague
	output_buffering: 4096

# Nette section
di:
	debugger: false

tracy:
	logSeverity: E_ALL
	strictMode: yes

application:
    mapping:
        Front: [App\UI\Modules\Front, *, *\*Presenter]