{
  "name": "marng/astro-forecast",
  "description": "Astronomy weather forecast",
  "keywords": [
    "php",
    "nette",
    "contributte",
    "api",
    "apitte"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.3",

    "contributte/bootstrap": "^0.7.0",
    "contributte/application": "^0.6.0",
    "contributte/cache": "^0.7.0",
    "contributte/console": "^0.11.0",
    "contributte/console-extra": "^0.8.0",
    "contributte/di": "^0.6.0",
    "contributte/forms": "^0.5.0",
    "contributte/http": "^0.5.0",
    "contributte/latte": "^0.6.0",
    "contributte/mail": "^0.8.0",
    "contributte/mailing": "^0.7.0",
    "contributte/monolog": "^0.6.0",
    "contributte/security": "^0.4.0",
    "contributte/utils": "^0.7.0",
    "contributte/tracy": "^0.6.0",
    "contributte/pdf": "^8.0.0",
    "contributte/apitte": "^0.13.0",

    "symfony/serializer": "^7.2.3",
    "symfony/validator": "^7.2.3",
    "symfony/property-info": "^7.2.3",
    "symfony/cache": "^7.2.3"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/dev": "^0.5",
    "contributte/tester": "^0.4",
    "contributte/phpstan": "^0.3",
    "mockery/mockery": "^1.6.12",
    "phpstan/phpstan-doctrine": "^2.0.0",
    "nelmio/alice": "^3.14.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "App"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
